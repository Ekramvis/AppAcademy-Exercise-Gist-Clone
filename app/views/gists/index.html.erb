<div id="main"></div>

<script type="text/javascript">
  var allGists = <%= @gists.to_json.html_safe %>
  var $main = $('#main')
  var user = <%= User.includes(:favorites).find_by_session_token(cookies["session_token"]).to_json(include: :favorites).html_safe %>

  $(function() {
    window.G.initialize(allGists, $main, user);
  });
</script>